<cfapplication name="Expense" sessionmanagement="yes" sessiontimeout="#CreateTimeSpan(0,0,30,0)#"><cflock scope="application" timeout="20" type="exclusive">	<cfparam name="application.data" default="#StructNew()#">	<cfset request.application=Duplicate(application.data)></cflock><cflock scope="session" timeout="20" type="exclusive">	<cfparam name="session.data" default="#StructNew()#">	<cfset request.session=Duplicate(session.data)></cflock><cfparam name="request.session.intDay" default="#Day(Now())#" type="numeric"><cfparam name="request.session.intMonth" default="#Month(Now())#" type="numeric"><cfparam name="request.session.intYear" default="#Year(Now())#" type="numeric">