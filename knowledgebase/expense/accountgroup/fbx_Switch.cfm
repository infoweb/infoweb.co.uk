<!---<fusedoc fuse="FBX_Switch.cfm">	<responsibilities>		I am the cfswitch statement that handles the fuseaction, delegating work to various fuses.	</responsibilities>	<io>		<string name="fusebox.fuseaction" />		<string name="fusebox.circuit" />	</io>	</fusedoc>---><cfswitch expression = "#fusebox.fuseaction#">	<cfcase value="fusebox.defaultfuseaction">		<cfinclude template="sqlSelectAccountGroup.cfm">		<cfif NOT rstaccountGroupList.recordCount>			<cflocation url="#selfFA#accountGroup.New" addtoken="no">		</cfif>		<cfset xfa.formAction="#selfFA#accountGroup.New">		<cfinclude template="dspDefault.cfm">	</cfcase>	<cfcase value="new">		<cfinclude template="sqlNewAccountGroup.cfm">		<cfset xfa.formAction="#selfFA#accountGroup.insert">		<cfset xfa.cancelAction="#selfFA#accountGroup.">		<cfinclude template="dspNewEdit.cfm">	</cfcase>	<cfcase value="edit">		<cfset strRecordsetName="rstAccountGroup">		<cfset strWhereString="pkiAccountGroupID=#attributes.id#">		<cfinclude template="sqlSelectAccountGroup.cfm">		<cfset xfa.formAction="#selfFA#accountGroup.update">		<cfset xfa.cancelAction="#selfFA#accountGroup.">		<cfinclude template="dspNewEdit.cfm">	</cfcase>	<cfcase value="insert">		<cfinclude template="sqlInsertAccountGroup.cfm">		<cflocation url="#selfFA#accountGroup.New" addtoken="no">	</cfcase>	<cfcase value="update">		<cfinclude template="sqlUpdateAccountGroup.cfm">		<cflocation url="#selfFA#accountGroup." addtoken="no">	</cfcase>	<cfcase value="delete">		<cfinclude template="sqlDeleteRecord.cfm">	</cfcase>	<cfdefaultcase>		This is the cfdefaultcase tag. I received a fuseaction called 		"<cfoutput>#attributes.fuseaction#</cfoutput>" and I don't know what to do with it.	</cfdefaultcase></cfswitch>